{
  "name": "apie/storage-metadata-builder",
  "description": "Composer package of the apie library: storage metadata builder",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\StorageMetadataBuilder\\": "src/"
    }
